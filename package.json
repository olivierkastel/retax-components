  "description": "Set of components to structure retax apps",
  "main": "lib/index.js",
  "jsnext:main": "es/index.js",
  "typings": "libTs/index.d.ts",
  "scripts": {
    "build": "builder run build",
    "lint": "builder run lint",
    "prepublish": "npm test && builder run build",
