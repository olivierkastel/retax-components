    "hoist-non-react-statics": "^1.0.5",
    "inversify": "2.0.0-beta.10",
    "inversify-dts": "1.0.8",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "retax-core": "^1.2.1",
    "retax-di": "^1.1.2",
    "retax-utils": "^1.0.2"
  },
  "devDependencies": {
